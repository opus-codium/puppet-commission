{
  "name": "opuscodium-commission",
  "version": "0.0.0",
  "author": "Opus Codium",
  "summary": "Commission / decommission Puppet nodes easily",
  "license": "MIT",
  "source": "https://github.com/opus-codium/puppet-commission",
  "project_page": "https://github.com/opus-codium/puppet-commission",
  "issues_url": "https://github.com/opus-codium/puppet-commission/issues",
  "dependencies": [
    {
      "name": "puppetlabs/ruby_task_helper",
      "version_requirement": ">= 0.6.0 < 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9",
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
